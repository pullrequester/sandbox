<?xml version="1.0" encoding="UTF-8"?>
<phpunit
  colors="true">
    <testsuite name="QuickBotTest">
      <file>QuickBotTest.php</file>
      <logging>
        <log type="coverage-clover" target="clover.xml" />
      </logging>
    </testsuite>
    <filter>
      <whitelist processUncoveredFilesFromWhitelist="true">
        <file>QuickBot.php</file>
        <exclude>
          <directory suffix=".php">twitteroauth/</directory>
        </exclude>
      </whitelist>
    </filter>
</phpunit>
